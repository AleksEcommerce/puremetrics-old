    "dd608887-5e45-455c-b9cc-0ba4918f0d68": {
      "type": "rich-text",
      "blocks": {
        "template--17923918430474__dd608887-5e45-455c-b9cc-0ba4918f0d68-1689161293b60e8094-0": {
          "type": "heading",
          "settings": {
            "title": "Birgit von der AGA Artenschutz sagt dazu"
